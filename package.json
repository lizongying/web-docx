{
  "name": "web-docx",
  "version": "0.1.0",
  "description": "HTML to DOCX converter",
  "keywords": [
    "html",
    "docx",
    "html-to-docx",
    "html to docx",
    "office",
    "word"
  ],
  "type": "module",
  "main": "dist/web-docx.umd.js",
  "module": "dist/web-docx.esm.js",
  "exports": {
    ".": {
      "import": "./dist/web-docx.esm.js",
      "require": "./dist/web-docx.umd.js"
    }
  },
  "types": "./src/types/web-docx.d.ts",
  "scripts": {
    "test0": "node example/example0.js",
    "test1": "node example/example1.ts",
    "release": "standard-version",
    "lint": "eslint --fix .",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lizongying/web-docx.git"
  },
  "author": "Li ZongYing <lizongying@msn.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lizongying/web-docx/issues"
  },
  "homepage": "https://github.com/lizongying/web-docx#readme",
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.2.0",
    "html-minifier-terser": "^7.2.0",
